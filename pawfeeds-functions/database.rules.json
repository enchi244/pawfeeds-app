{
  "rules": {
    "user_notifications": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid",

        ".indexOn": "timestamp"
      }
    },
    "commands": {
      "$feederId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    }
  }
}